    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/axios": "^0.14.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.1",
    "@types/leaflet": "^1.5.12",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-leaflet-markercluster": "^2.0.0",
    "@types/react-redux": "^7.1.7",
    "@types/redux": "^3.6.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.4.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "raf": "^3.4.1",
    "style-loader": "^1.1.4",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "geojson-world-map": "0.0.2",
    "leaflet": "^1.6.0",
    "leaflet.markercluster": "^1.4.1",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-leaflet": "^2.6.3",
    "react-leaflet-markercluster": "^2.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "setupFiles": [
      "<rootDir>/config/test/polyfills.js",
      "<rootDir>/config/test/setupTest.js"
    ],
    "restoreMocks": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
