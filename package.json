  "description": "React map with Leaflet",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "start-dev": "webpack-dev-server  --mode development --inline --hot --open",
    "build": "webpack -p",
    "watch": "webpack --watch",
    "storybook": "start-storybook -p 6006",
